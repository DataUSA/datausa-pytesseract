    <CalculatedMeasure name="Area sq miles" dimension="Measures" visible="false">
      <Formula> CASE
            WHEN [Geography].[Tract].Level is [Geography].[Tract].[Tract] THEN (cast([Geography].[Tract].CurrentMember.Properties("Area") as integer) / 2589988)
